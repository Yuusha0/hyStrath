FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      B;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -2 0 0 -1 0];

internalField   uniform (0 0 0);

boundaryField
{    
	Inlet
	{
		type            fixedValue;
		value           uniform (0 0 0);
	}
	periodic_1
	{
		type            cyclic;
	}
	periodic_2
	{
		type            cyclic;
	}
	Wall
	{
		type            fixedValue;
		value           uniform (0 0 0);
	}
	front
	{
		type            empty;
	}
	back
	{
	 	type            empty;
	}

}


