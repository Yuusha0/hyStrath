FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      fD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    	Inlet
	{
		type            zeroGradient;
	}
	periodic_1
	{
		type            cyclic;
	}
	periodic_2
	{
		type            cyclic;
	}
	Wall
	{
		type            calculated;
		value           uniform (0 0 0);
	}
	front
	{
		type            empty;
	}
	back
	{
	 	type            empty;
	}
}


